-- // MASTER TOGGLE (Satu saklar untuk semua)
_G.Master_ObjectESP = false 
local ESP_Tracers = false

-- // CONFIGURATION TABLE (Pusat kendali tiap object)
local TargetConfigs = {
    -- Items
    ["GoldPile"] = {
        DisplayName = "Coin", 
        Color = Color3.fromRGB(255, 251, 39), 
        Enabled = false -- Bisa dimatikan khusus yang ini saja
    },
    ["KeyObtain"] = {
        DisplayName = "Key", 
        Color = Color3.fromRGB(255, 251, 39), 
        Enabled = false -- Bisa dimatikan khusus yang ini saja
    },
    --Object can action
    ["Wardrobe"] = {
        DisplayName = "Wardrobe", 
        Color = Color3.fromRGB(16, 227, 255), 
        Enabled = false
    },
    ["Door"] = {
        DisplayName = "Door", 
        Color = Color3.fromRGB(16, 227, 255), 
        Enabled = false -- Contoh: Medkit sedang tidak ingin dilacak
    },
    ["Bed"] = {
        DisplayName = "Bed", 
        Color = Color3.fromRGB(16, 227, 255), 
        Enabled = false -- Contoh: Medkit sedang tidak ingin dilacak
    },
