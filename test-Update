local SpeedValue = 1
local SpeedEnable = false

task.spawn(function()
    while true do
        task.wait(0.1)

        if SpeedEnable then
            Character:SetAttribute("SpeedBoostBehind", SpeedValue)
        else
            Character:SetAttribute("SpeedBoostBehind", 0)
        end
    end
end)
---------------------------------------------------------------------------
---------------------------------------------------------------------------
local AntiAFK = false

task.spawn(function()
    while true do
        task.wait(0.1)

        if AntiAFK then
            if Players:SetAttribute("AFK", true) then
                Players:SetAttribute("AFK", false)
            end
        end
    end
end)
