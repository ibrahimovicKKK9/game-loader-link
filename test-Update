local SetFOVValue = 100
local EnableFOV = false

local function FieldOfViewCustom()
    if EnableFOV then
        Camera.FieldOfView = SetFOVValue
    else
        Camera.FieldOfView = 70
    end
end

Camera:GetPropertyChangedSignal("FieldOfView"):Connect(FieldOfViewCustom)
